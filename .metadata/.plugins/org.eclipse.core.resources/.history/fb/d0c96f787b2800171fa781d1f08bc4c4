
public class Tarea {
	
	private String nombreTarea,
				   fechaLimite;
	
	private Materia nombreMateria;

	public Tarea(){
		this.nombreTarea = "";
		this.fechaLimite = "";
		//this.nombreMateria = nombreMateria;
	}

	// Getters
	public String getNombreTarea(){
		return this.nombreTarea;
	}
	
	public String getFechaLimite(){
		return this.fechaLimite;	
	}
	
}
